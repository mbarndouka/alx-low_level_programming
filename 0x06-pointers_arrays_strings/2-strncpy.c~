#include "main.h"
#include <stdio.h>

/**
 *_strcpy - function that copy a sting
 *@dest: input value
 *@src: input value
 *@n: input value
 *Return: void
 */

char *_strncpy(char *dest, char *src, int n)
{
char *ptr = dest;
int i = 0;
while (i < n && *src != '\0')
{
*ptr++ = *src++;
n++;
}
while (n > i)
{
*ptr++ = '\0';
n++;
}
return (dest);
}
